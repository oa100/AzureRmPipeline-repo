{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "[format('fnapp{0}', uniqueString(resourceGroup().id))]"
    },
    "storageAccountType": {
      "value": "Standard_LRS"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "appInsightsLocation": {
      "value": "Central US"
    },
    "runtime": {
      "value": "node"
    }
  }
}